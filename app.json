{
    "builds": {
        "web": {
            "default": true
        },
        "android": {
            "packager": "cordova",
            "cordova": {
                "config": {
                    "platforms": "android",
                    "id": "com.emisia.fss",
                    "name": "FSS"
                }
            },
            "theme": "fssmaterial"
        }
    },
    "name": "FSS",
    "namespace": "",
    "version": "1.0.0.0",
    "framework": "ext",
    "toolkit": "modern",
    "theme": "fssmaterial",
    "locale": "sr",
    "indexHtmlPath": "index.html",
    "classpath": [
        "app"
    ],
    "overrides": [
        "overrides"
    ],
    "fashion": {
        "missingParameters": "error",
        "inliner": {
            "enable": false
        }
    },
    "sass": {
        "namespace": "",
        "generated": {
            "var": "sass/save.scss",
            "src": "sass/save"
        },
        "etc": [
            "sass/etc/all.scss"
        ],
        "var": [
            "sass/var/all.scss",
            "sass/var"
        ],
        "src": [
            "sass/src"
        ]
    },
    "js": [
        {
            "path": "resources/3rdparty/firebase/firebase.js"
        },
        {
            "path": "app.js",
            "bundle": true
        }
    ],
    "css": [
        {
            "path": "${build.out.css.path}",
            "bundle": true,
            "exclude": [
                "fashion"
            ]
        }
    ],
    "loader": {
        "cache": false,
        "cacheParam": "_dc"
    },
    "production": {
        "output": {
            "appCache": {
                "enable": true,
                "path": "cache.appcache"
            }
        },
        "loader": {
            "cache": "${build.timestamp}"
        },
        "cache": {
            "enable": true
        },
        "compressor": {
            "type": "yui"
        }
    },
    "testing": {},
    "development": {
        "watch": {
            "delay": 250
        }
    },
    "bootstrap": {
        "base": "${app.dir}",
        "microloader": "bootstrap.js",
        "css": "bootstrap.css"
    },
    "output": {
        "base": "${workspace.build.dir}/${build.environment}/${app.name}",
        "appCache": {
            "enable": false
        }
    },
    "cache": {
        "enable": false,
        "deltas": true
    },
    "appCache": {
        "cache": [
            "index.html",
            "login.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },
    "resources": [
        {
            "path": "resources",
            "output": "shared"
        }
    ],
    "archivePath": "archive",
    "slicer": null,
    "ignore": [
        "(^|/)CVS(/?$|/.*?$)"
    ],
    "id": "e808b5ad-3dda-4ef0-b612-65086b013d16"
}